# Server
NODE_ENV=development
PORT=3005
LOG_LEVEL=info

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5436/analytics_engine

# YouTube Data API v3 (read-only: views, likes, comments)
YOUTUBE_API_KEY=your-youtube-api-key-here

# YouTube Analytics API OAuth2 (CPM, revenue, watch time)
# Get via: https://console.cloud.google.com/apis/credentials
YOUTUBE_OAUTH_TOKEN=
YOUTUBE_REFRESH_TOKEN=
YOUTUBE_CLIENT_ID=
YOUTUBE_CLIENT_SECRET=

# Internal services
SCRIPT_ENGINE_URL=http://localhost:3002

# Sync settings
SYNC_INTERVAL_HOURS=24
