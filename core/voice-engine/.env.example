# Server
NODE_ENV=development
PORT=3003
LOG_LEVEL=info
BASE_URL=http://localhost:3003

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5434/voice_engine

# ElevenLabs
ELEVENLABS_API_KEY=your-elevenlabs-key-here
ELEVENLABS_MODEL=eleven_multilingual_v2

# Storage (local for dev, replace with S3 URL for prod)
UPLOAD_DIR=./uploads/audio
MAX_CHARS_PER_CHUNK=4500

# Internal services
SCRIPT_ENGINE_URL=http://localhost:3002
